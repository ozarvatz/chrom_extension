{
    "manifest_version": 2,
    "name": "HelloWorld",
    "description": "This extension injects hello world into every webpage you visit",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs",
        "bookmarks"
    ],
    "background": {
        "scripts": [ "background.js" ]
    },
    "content_scripts": [
        {
            "matches": [ "http://images1.ynet.co.il/PicServer3/2013/07/13/4733181/47330573074894408529no.jpg" ],
            "js": [ "content.js" ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "OFF"
    }
}
